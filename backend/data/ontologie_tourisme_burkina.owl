<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:onto="http://www.owl-ontologie-tourisme#"
>
  <!-- DÉFINITION DES CLASSES -->
  <owl:Class rdf:about="http://www.owl-ontologie-tourisme#Activite"/>
  <owl:Class rdf:about="http://www.owl-ontologie-tourisme#Localite"/>
  <owl:Class rdf:about="http://www.owl-ontologie-tourisme#SiteTouristique"/>

  <!-- PROPRIÉTÉS D'OBJET -->
  <owl:ObjectProperty rdf:about="http://www.owl-ontologie-tourisme#situeDans">
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#SiteTouristique"/>
    <rdfs:range rdf:resource="http://www.owl-ontologie-tourisme#Localite"/>
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:about="http://www.owl-ontologie-tourisme#proposeActivite">
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#SiteTouristique"/>
    <rdfs:range rdf:resource="http://www.owl-ontologie-tourisme#Activite"/>
  </owl:ObjectProperty>

  <!-- PROPRIÉTÉS DE DONNÉES -->
  <owl:DatatypeProperty rdf:about="http://www.owl-ontologie-tourisme#periodeRecommandee">
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#SiteTouristique"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:about="http://www.owl-ontologie-tourisme#type">
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#Activite"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:about="http://www.owl-ontologie-tourisme#coordonneesGPS">
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#Localite"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:about="http://www.owl-ontologie-tourisme#accessibilite">
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#SiteTouristique"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:about="http://www.owl-ontologie-tourisme#statut">
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#SiteTouristique"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:about="http://www.owl-ontologie-tourisme#nom">
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#SiteTouristique"/>
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#Activite"/>
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#Localite"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:about="http://www.owl-ontologie-tourisme#description">
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#SiteTouristique"/>
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#Activite"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:about="http://www.owl-ontologie-tourisme#typeSite">
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#SiteTouristique"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  
  <owl:DatatypeProperty rdf:about="http://www.owl-ontologie-tourisme#typeLocalite">
    <rdfs:domain rdf:resource="http://www.owl-ontologie-tourisme#Localite"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>

  <!-- INSTANCES - LOCALITÉS -->
  
  <!-- Ouagadougou -->
  <onto:Localite rdf:about="http://www.owl-ontologie-tourisme#Ouagadougou">
    <onto:nom>Ouagadougou</onto:nom>
    <onto:typeLocalite>Capitale</onto:typeLocalite>
    <onto:coordonneesGPS>12.3714, -1.5197</onto:coordonneesGPS>
  </onto:Localite>

  <!-- Bobo-Dioulasso -->
  <onto:Localite rdf:about="http://www.owl-ontologie-tourisme#BoboDioulasso">
    <onto:nom>Bobo-Dioulasso</onto:nom>
    <onto:typeLocalite>Ville</onto:typeLocalite>
    <onto:coordonneesGPS>11.1781, -4.2977</onto:coordonneesGPS>
  </onto:Localite>

  <!-- Banfora -->
  <onto:Localite rdf:about="http://www.owl-ontologie-tourisme#Banfora">
    <onto:nom>Banfora</onto:nom>
    <onto:typeLocalite>Ville</onto:typeLocalite>
    <onto:coordonneesGPS>10.6333, -4.7667</onto:coordonneesGPS>
  </onto:Localite>

  <!-- Arli -->
  <onto:Localite rdf:about="http://www.owl-ontologie-tourisme#Arli">
    <onto:nom>Arli</onto:nom>
    <onto:typeLocalite>Parc national</onto:typeLocalite>
    <onto:coordonneesGPS>11.5000, 1.2500</onto:coordonneesGPS>
  </onto:Localite>

  <!-- Tiébélé -->
  <onto:Localite rdf:about="http://www.owl-ontologie-tourisme#Tiebele">
    <onto:nom>Tiébélé</onto:nom>
    <onto:typeLocalite>Village</onto:typeLocalite>
    <onto:coordonneesGPS>11.1667, -0.8333</onto:coordonneesGPS>
  </onto:Localite>

  <!-- Loropéni -->
  <onto:Localite rdf:about="http://www.owl-ontologie-tourisme#Loropeni">
    <onto:nom>Loropéni</onto:nom>
    <onto:typeLocalite>Site archéologique</onto:typeLocalite>
    <onto:coordonneesGPS>10.2833, -3.5167</onto:coordonneesGPS>
  </onto:Localite>

  <!-- INSTANCES - ACTIVITÉS -->
  
  <!-- Safari -->
  <onto:Activite rdf:about="http://www.owl-ontologie-tourisme#Safari">
    <onto:nom>Safari</onto:nom>
    <onto:type>Nature</onto:type>
    <onto:description>Observation de la faune sauvage dans son habitat naturel</onto:description>
  </onto:Activite>

  <!-- Visite culturelle -->
  <onto:Activite rdf:about="http://www.owl-ontologie-tourisme#VisiteCulturelle">
    <onto:nom>Visite culturelle</onto:nom>
    <onto:type>Culture</onto:type>
    <onto:description>Découverte du patrimoine culturel et architectural traditionnel</onto:description>
  </onto:Activite>

  <!-- Randonnée -->
  <onto:Activite rdf:about="http://www.owl-ontologie-tourisme#Randonnee">
    <onto:nom>Randonnée</onto:nom>
    <onto:type>Sport/Nature</onto:type>
    <onto:description>Marche et exploration des paysages naturels</onto:description>
  </onto:Activite>

  <!-- Photographie -->
  <onto:Activite rdf:about="http://www.owl-ontologie-tourisme#Photographie">
    <onto:nom>Photographie</onto:nom>
    <onto:type>Loisir</onto:type>
    <onto:description>Capture d'images des paysages et de la culture locale</onto:description>
  </onto:Activite>

  <!-- Baignade -->
  <onto:Activite rdf:about="http://www.owl-ontologie-tourisme#Baignade">
    <onto:nom>Baignade</onto:nom>
    <onto:type>Loisir</onto:type>
    <onto:description>Activité aquatique dans les cascades et plans d'eau naturels</onto:description>
  </onto:Activite>

  <!-- Visite archéologique -->
  <onto:Activite rdf:about="http://www.owl-ontologie-tourisme#VisiteArcheologique">
    <onto:nom>Visite archéologique</onto:nom>
    <onto:type>Culture/Histoire</onto:type>
    <onto:description>Exploration de sites historiques et vestiges anciens</onto:description>
  </onto:Activite>

  <!-- INSTANCES - SITES TOURISTIQUES -->

  <!-- Parc National d'Arli -->
  <onto:SiteTouristique rdf:about="http://www.owl-ontologie-tourisme#ParcNationalArli">
    <onto:nom>Parc National d'Arli</onto:nom>
    <onto:description>Parc national situé dans l'est du Burkina Faso, connu pour sa faune diversifiée incluant éléphants, lions, buffles et nombreuses espèces d'oiseaux</onto:description>
    <onto:typeSite>Parc national</onto:typeSite>
    <onto:statut>Aire protégée</onto:statut>
    <onto:periodeRecommandee>Novembre à Mars (saison sèche)</onto:periodeRecommandee>
    <onto:accessibilite>Accessible en véhicule 4x4, guides obligatoires</onto:accessibilite>
    <onto:situeDans rdf:resource="http://www.owl-ontologie-tourisme#Arli"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Safari"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Photographie"/>
  </onto:SiteTouristique>

  <!-- Cascades de Karfiguéla -->
  <onto:SiteTouristique rdf:about="http://www.owl-ontologie-tourisme#CascadesKarfiguela">
    <onto:nom>Cascades de Karfiguéla</onto:nom>
    <onto:description>Magnifiques chutes d'eau situées près de Banfora, site naturel spectaculaire avec plusieurs niveaux de cascades dans un cadre verdoyant</onto:description>
    <onto:typeSite>Site naturel</onto:typeSite>
    <onto:statut>Site touristique</onto:statut>
    <onto:periodeRecommandee>Juin à Octobre (saison des pluies pour le débit maximum)</onto:periodeRecommandee>
    <onto:accessibilite>Accessible en voiture, puis à pied (sentier aménagé)</onto:accessibilite>
    <onto:situeDans rdf:resource="http://www.owl-ontologie-tourisme#Banfora"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Randonnee"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Photographie"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Baignade"/>
  </onto:SiteTouristique>

  <!-- Dômes de Fabédougou -->
  <onto:SiteTouristique rdf:about="http://www.owl-ontologie-tourisme#DomesFabedougou">
    <onto:nom>Dômes de Fabédougou</onto:nom>
    <onto:description>Formation rocheuse unique constituée de dômes de grès sculptés par l'érosion, offrant un paysage lunaire exceptionnel</onto:description>
    <onto:typeSite>Formation géologique</onto:typeSite>
    <onto:statut>Site naturel protégé</onto:statut>
    <onto:periodeRecommandee>Novembre à Mars (température plus clémente)</onto:periodeRecommandee>
    <onto:accessibilite>Accessible en véhicule, escalade possible avec guide</onto:accessibilite>
    <onto:situeDans rdf:resource="http://www.owl-ontologie-tourisme#Banfora"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Randonnee"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Photographie"/>
  </onto:SiteTouristique>

  <!-- Village de Tiébélé -->
  <onto:SiteTouristique rdf:about="http://www.owl-ontologie-tourisme#VillageTiebele">
    <onto:nom>Village de Tiébélé</onto:nom>
    <onto:description>Village traditionnel Kassena célèbre pour ses maisons décorées de motifs géométriques peints par les femmes, patrimoine architectural unique</onto:description>
    <onto:typeSite>Site culturel</onto:typeSite>
    <onto:statut>Patrimoine culturel</onto:statut>
    <onto:periodeRecommandee>Toute l'année, éviter la saison des pluies pour les décorations</onto:periodeRecommandee>
    <onto:accessibilite>Accessible en véhicule, visite avec guide local recommandée</onto:accessibilite>
    <onto:situeDans rdf:resource="http://www.owl-ontologie-tourisme#Tiebele"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#VisiteCulturelle"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Photographie"/>
  </onto:SiteTouristique>

  <!-- Grande Mosquée de Bobo-Dioulasso -->
  <onto:SiteTouristique rdf:about="http://www.owl-ontologie-tourisme#GrandeMosqueeBoboDioulasso">
    <onto:nom>Grande Mosquée de Bobo-Dioulasso</onto:nom>
    <onto:description>Magnifique exemple d'architecture soudano-sahélienne, construite en banco, l'une des plus belles mosquées d'Afrique de l'Ouest</onto:description>
    <onto:typeSite>Monument religieux</onto:typeSite>
    <onto:statut>Monument historique</onto:statut>
    <onto:periodeRecommandee>Toute l'année, respecter les heures de prière</onto:periodeRecommandee>
    <onto:accessibilite>Accessible à pied, centre-ville de Bobo-Dioulasso</onto:accessibilite>
    <onto:situeDans rdf:resource="http://www.owl-ontologie-tourisme#BoboDioulasso"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#VisiteCulturelle"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Photographie"/>
  </onto:SiteTouristique>

  <!-- Musée National du Burkina Faso -->
  <onto:SiteTouristique rdf:about="http://www.owl-ontologie-tourisme#MuseeNationalBurkinaFaso">
    <onto:nom>Musée National du Burkina Faso</onto:nom>
    <onto:description>Principal musée du pays présentant l'histoire, la culture et l'artisanat traditionnel burkinabé à travers des collections d'objets ethnographiques</onto:description>
    <onto:typeSite>Musée</onto:typeSite>
    <onto:statut>Institution culturelle nationale</onto:statut>
    <onto:periodeRecommandee>Toute l'année, heures d'ouverture variables</onto:periodeRecommandee>
    <onto:accessibilite>Accessible en transport urbain, centre de Ouagadougou</onto:accessibilite>
    <onto:situeDans rdf:resource="http://www.owl-ontologie-tourisme#Ouagadougou"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#VisiteCulturelle"/>
  </onto:SiteTouristique>

  <!-- Ruines de Loropéni -->
  <onto:SiteTouristique rdf:about="http://www.owl-ontologie-tourisme#RuinesLoropeni">
    <onto:nom>Ruines de Loropéni</onto:nom>
    <onto:description>Vestiges de fortifications en pierre datant du 11ème au 19ème siècle, inscrites au patrimoine mondial de l'UNESCO depuis 2009</onto:description>
    <onto:typeSite>Site archéologique</onto:typeSite>
    <onto:statut>Patrimoine mondial UNESCO</onto:statut>
    <onto:periodeRecommandee>Novembre à Mars (saison sèche)</onto:periodeRecommandee>
    <onto:accessibilite>Accessible en véhicule, visite guidée recommandée</onto:accessibilite>
    <onto:situeDans rdf:resource="http://www.owl-ontologie-tourisme#Loropeni"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#VisiteArcheologique"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#VisiteCulturelle"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Photographie"/>
  </onto:SiteTouristique>

  <!-- Lac de Tengrela -->
  <onto:SiteTouristique rdf:about="http://www.owl-ontologie-tourisme#LacTengrela">
    <onto:nom>Lac de Tengrela</onto:nom>
    <onto:description>Lac artificiel près de Banfora, réputé pour ses hippopotames visibles depuis la rive et ses magnifiques couchers de soleil</onto:description>
    <onto:typeSite>Plan d'eau</onto:typeSite>
    <onto:statut>Site touristique</onto:statut>
    <onto:periodeRecommandee>Novembre à Mai (saison sèche pour observer les hippopotames)</onto:periodeRecommandee>
    <onto:accessibilite>Accessible en véhicule, observation depuis la berge</onto:accessibilite>
    <onto:situeDans rdf:resource="http://www.owl-ontologie-tourisme#Banfora"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Safari"/>
    <onto:proposeActivite rdf:resource="http://www.owl-ontologie-tourisme#Photographie"/>
  </onto:SiteTouristique>

</rdf:RDF>